{
  "name": "react-input-toggle",
  "version": "0.2.0",
  "description": "A collection of aesthetically pleasing, accessible and customizable toggles build with React.",
  "keywords": [
    "react",
    "react-component",
    "toggle",
    "switch",
    "input",
    "checkbox",
    "react-input"
  ],
  "main": "./dist/react-input-toggle.js",
  "repository": {
    "type": "git",
    "url": "github.com/enjoylife/react-input-toggle.git"
  },
  "bugs": "github.com/enjoylife/react-input-toggle/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "NODE_ENV=production TARGET=minify webpack --config webpack.prod.config.js",
    "prebuild": "rm -rf dist && mkdir dist",
    "test": "karma start --browsers Firefox --single-run",
    "test:dev": "karma start",
    "cov": "codecov < ./coverage/lcov/lcov.info",
    "dev": "webpack-dev-server --inline --progress --colors --config -./webpack.config.js --host 0.0.0.0 --content-base ./"
  },
  "authors": [
    "Matthew Clemens"
  ],
  "license": "MIT",
