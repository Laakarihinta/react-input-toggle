{
  "name": "react-input-toggle",
  "version": "0.1.0",
  "description": "A collection of aesthetically pleasing, accessible and customizable toggles build with React.",
  "keywords": [
    "react",
    "react-component",
    "toggle",
    "switch",
    "input",
    "checkbox",
    "react-input"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "github.com/enjoylife/react-input-toggle.git"
  },
  "bugs": "github.com/enjoylife/react-input-toggle/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "NODE_ENV=production TARGET=minify webpack --config webpack.prod.config.js",
    "prebuild": "rm -rf dist && mkdir dist",
    "prepublish": "npm run build",
    "dev": "webpack-dev-server --inline --progress --colors --config -./webpack.config.js --host 0.0.0.0 --content-base examples/",
    "test-w": "mocha --recursive  --watch --compilers jsx:babel-core/register test/*.jsx"
  },
  "authors": [
    "Matthew Clemens"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.1.4",
    "babel-helper-builder-react-jsx": "^6.0.15",
    "babel-loader": "^6.0.1",
    "babel-plugin-syntax-class-properties": "^6.0.14",
    "babel-plugin-syntax-object-rest-spread": "^6.0.14",
    "babel-plugin-transform-class-constructor-call": "^6.0.20",
    "babel-plugin-transform-class-properties": "^6.0.14",
    "babel-plugin-transform-function-bind": "^6.1.4",
    "babel-plugin-transform-react-jsx": "^6.0.18",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "classnames": "^2.2.0",
    "css-loader": "^0.22.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.1",
    "react": "^0.14.2",
    "react-a11y": "^0.2.8",
    "react-addons-pure-render-mixin": "^0.14.2",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": "^0.14.2",
    "sass-loader": "^3.1.1",
    "should": "^7.1.1",
    "should-sinon": "0.0.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.12.1",
    "webpack-error-notification": "^0.1.4"
  },
  "peerDependencies": {
    "react": "0.14"
  },
  "dependencies": {}
}
