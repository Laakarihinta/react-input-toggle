    "babel-plugin-syntax-class-properties": "^6.0.14",
    "babel-plugin-syntax-object-rest-spread": "^6.0.14",
    "babel-plugin-transform-class-constructor-call": "^6.0.20",
    "babel-plugin-transform-class-properties": "^6.0.14",
    "babel-plugin-transform-function-bind": "^6.1.4",
    "babel-plugin-transform-react-jsx": "^6.0.18",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "chai": "^3.4.1",
    "chai-spies": "^0.7.1",
    "classnames": "^2.2.0",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.22.0",
    "extract-text-webpack-plugin": "^0.9.1",
